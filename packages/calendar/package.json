{
  "name": "@hectorbliss/denik-calendar",
  "version": "0.0.5",
  "private": false,
  "description": "A React drag-and-drop week calendar component with overlap detection. Built with @dnd-kit.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "react-component",
    "calendar",
    "week-calendar",
    "drag-and-drop",
    "dnd-kit",
    "scheduling",
    "agenda",
    "booking"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blissito/denik-calendar"
  },
  "homepage": "https://github.com/blissito/denik-calendar#readme"
}
